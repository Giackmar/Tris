<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tris">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".Singleplayer" android:theme="@style/AppTheme" android:parentActivityName="com.example.tris.StartActivity" >></activity>
        <activity
            android:name=".StartActivity"
            android:theme="@style/StartTheme"
            android:launchMode="singleTop">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".MultiplayerActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName="com.example.tris.StartActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>